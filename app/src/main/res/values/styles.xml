<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="holdingsHeader">
        <item name="android:textColor">@color/dark_grey</item>
        <item name="android:textSize">32sp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>
        <!--        <item name="android:background">@color/grey</item>-->

    </style>

    <style name="quantity_bold">
        <item name="android:textColor">#2A2A2A</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="SearchMenuTab" parent="android:Widget.TextView">
        <item name="android:textColor">@color/bright_foreground_disabled_material_dark</item>
        <item name="android:textSize">@dimen/sp14</item>
        <item name="textAllCaps">true</item>
        <item name="android:letterSpacing">0.14</item>
    </style>

    <style name="symbol">
        <item name="android:textColor">#2A2A2A</item>
        <item name="android:textSize">@dimen/sp16</item>
        <item name="android:fontFamily">sans-serif-black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="text_heading">
        <item name="android:textColor">#2A2A2A</item>
        <item name="android:textSize">@dimen/sp36</item>
        <item name="android:fontFamily">sans-serif-black</item>
        <item name="android:textStyle">bold</item>
    </style>


    <style name="text_description">
        <item name="android:textColor">#2A2A2A</item>
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">sans-serif-black</item>
        <item name="android:textStyle">normal</item>
    </style>


    <style name="quantity">
        <item name="android:textColor">#2A2A2A</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

</resources>